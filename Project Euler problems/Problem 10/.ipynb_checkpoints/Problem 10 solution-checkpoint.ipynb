    "def primeSum(n):\n",
    "    i = 2\n",
    "    list_of_primes = []\n",
    "    while i < n:\n",
    "        if all(i%prime for prime in list_of_primes):\n",
    "                list_of_primes.append(i)\n",
    "        i +=1\n",
    "    return sum(list_of_primes)\n",
    "            \n",
    "    \n",
    "primeSum(2000000)"
   ]
